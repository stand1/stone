/* pages/submitOrders/submitOrders.wxss */
.orders{
  width: 100%;
  box-sizing: border-box;
}
.ordersBox{
  width: 100%;
  height: 100%;
  position: fixed;
  box-sizing: border-box;
  background:rgba(249,249,248,1);
  padding: 21rpx 24rpx 120rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  overflow: auto;
}
.ordersBoxAddress{
  width: 100%;
  box-sizing: border-box;
  padding: 25rpx 27rpx;
  background: #fff;
  border-radius:8rpx;
}
.ordersBoxAddress .title{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
  border-bottom:2rpx dashed rgba(102,102,102,1);
  padding-bottom: 20rpx;
}
.ordersBoxAddress .title .changes{
  font-size:22rpx;
  color:rgba(238,107,17,1);
  padding: 10rpx;
}
.ordersBoxAddress .add{
  width: 100%;
  height: 156rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}
.ordersBoxAddress .add .addBtn{
  width:320rpx;
  height:62rpx;
  border:2rpx solid rgba(238,107,17,1);
  border-radius:31rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:28rpx;
  color:rgba(238,107,17,1);
}
.ordersBoxAddress .add .addBtn .addIcon{
  width: 30rpx;
  height: 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #EE6B11;
  color: #fff;
  margin-right: 20rpx;
  border-radius: 50%;
}
.ordersBoxAddress .address{
  width: 100%;
  box-sizing: border-box;
  padding-top: 22rpx;
}
.ordersBoxAddress .address view{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ordersBoxMsg{
  width: 100%;
  box-sizing: border-box;
  padding: 23rpx 27rpx 27rpx;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
  margin: 12rpx 0 14rpx;
}
.ordersBoxMsg .title{
  font-size:32rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.ordersBoxMsg .timers{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 18rpx;
  border-bottom:2rpx dashed rgba(102,102,102,1);
  margin-top: 16rpx;
}
.ordersBoxMsg .timers .timersTitle{
  font-size:26rpx;
  font-weight:bold;
  color:rgba(0,0,0,1);
}
.ordersBoxMsg .txtBox{
  width: 100%;
  box-sizing: border-box;
  margin-top: 19rpx;
}
.ordersBoxMsg .txtBox view{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ordersBoxInfo{
  margin-bottom: 14rpx;
}
.ordersBoxInfo .txtBox{
  padding-top: 5rpx;
}
.ordersBoxInfo .txtBox .txtBoxItem{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}
.command{
  width: 100%;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
  padding: 26rpx 27rpx 27rpx;
}
.command .title{
  width: 100%;
  box-sizing: border-box;
  font-size:32rpx;
  font-weight:bold;
  color:rgba(0,0,0,1);
  display: flex;
  align-items: center;
}
.command .title .titleIcon{
  width: 30rpx;
  height: 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  background:#7E7B7F;
  border-radius: 50%;
  margin-left: 12rpx;
}
.command .commandBox{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 28rpx;
  border-top:2rpx dashed rgba(102,102,102,1);
  margin-top: 19rpx;
}
.command .commandBox .verify{
  font-size:22rpx;
  text-decoration:underline;
  color:rgba(106,127,166,1);
}
.command .commandBox .verifyed{
  color: #EA7F65;
}
.explain{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  font-size:22rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.explain .explainIcon{
  width: 24rpx;
  height: 24rpx;
  box-sizing: border-box;
  border: 1rpx solid #f2f3f7;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  margin-right: 15rpx;
}
.explain .expColor{
  color: #EE6B11;
}
.btnBox{
  width: 100%;
  height:98rpx;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(229,229,229,1);
  box-sizing: border-box;
  padding: 0 24rpx 0 26rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(24,27,34,1);
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 99;
}
.btnBox .money{
  display: flex;
  align-items: center
}
.btnBox .money .moneyColor{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,95,0,1);
}
.btnBox .btn{
  width:200rpx;
  height:64rpx;
  background:rgba(255,95,0,1);
  border-radius:32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
}
.modleCommand{
  width: 100%;
  box-sizing: border-box;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(109,109,109,1);
  padding: 0 40rpx 0
}
.modleCommand .title{
  font-size:30rpx;
  font-weight:bold;
  color:rgba(0,0,0,1);
  padding: 37rpx 0 29rpx;
  border-bottom:1rpx solid rgba(149,149,149,1);
}
.modleCommand .txtCon{
  width: 100%;
  box-sizing: border-box;
  padding: 32rpx 0 108rpx;
  display: flex;
}
.modleCommand .txtConL{
  width:31rpx;
  height:31rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1rpx solid #EB6100;
  border-radius: 50%;
  color: #EB6100;
  margin-right: 27rpx;
}
.modleCommand .txtConR{
  text-align: left;
  width: 611rpx;
  box-sizing: border-box;
}
.modelVerify{
  width: 100%;
  height: 1100rpx;
  box-sizing: border-box;
  padding: 24rpx;
  background:rgba(249,249,248,1);
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.modelVerify .title{
  font-size:30rpx;
  font-weight:bold;
  color:rgba(0,0,0,1);
  padding: 27rpx 0 29rpx;
}
.modelVerify .verifyBox{
  width: 100%;
  box-sizing: border-box;
  text-align: left;
}
.modelVerify .verifyBox .verifyBoxInp{
  width: 100%;
  height:88rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
}
.modelVerify .verifyBox .verifyBoxInp input{
  width:700rpx;
  height:86rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  border: none;
  font-size:28rpx;
  font-weight:500;
  color:#333;
  padding-left: 20rpx;
  text-align: left;
}
.modelVerify .verifyBoxBtn{
  width:702rpx;
  height:64rpx;
  line-height: 64rpx;
  box-sizing: border-box;
  background:rgba(255,95,0,1);
  border-radius:32rpx 32rpx 32rpx 0px;
  margin-top: 26rpx;
  margin-bottom: 15rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  text-align: center;
}