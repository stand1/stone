/* pages/sharePosters/sharePosters.wxss */
.sharePostersBox{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: fixed;
  overflow: auto;
  background:rgba(249,249,248,1);
}
.sharePosters{
  width: 100%;
  box-sizing: border-box;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  padding: 56rpx 120rpx 236rpx;
}
.title{
  margin-bottom: 56rpx;
  display: flex;
  justify-content: center;
}
.posters{
  width: 510rpx;
  height: 834rpx;
  box-sizing: border-box;
}
.posters image{
  width: 510rpx;
  height: 834rpx;
}