/* pages/withdraw/withdraw.wxss */
.withdrawBox{
  width: 100%;
  height: 100%;
  position: fixed;
  box-sizing: border-box;
  overflow: auto;
  background:rgba(249,249,248,1);
}
.withdraw{
  width: 100%;
  box-sizing: border-box;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.withdrawT{
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  padding: 31rpx 24rpx 24rpx;
}
.withdrawT .header{
  width: 100%;
  font-weight:bold;
  color:rgba(82,92,122,1);
  display: flex;
  justify-content: flex-end;
  align-items: center;
  box-sizing: border-box;
  margin-bottom: 26rpx;
}
.center{
  width: 100%;
  height: 270rpx;
  box-sizing: border-box;
  background: url('https://yd.mycze.com/img/invitation/looseBg.png') no-repeat;
  background-size: 100% 100%;
  padding: 77rpx 49rpx 81rpx 44rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.centerL .centerLT{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(228,203,165,1);
}
.centerLT image{
  width: 35rpx;
  height: 24rpx;
  box-sizing: border-box;
  margin-left: 14rpx;
}
.centerLTColor{
  font-size:50rpx;
  font-weight:bold;
  margin-top: 18rpx;
}
.centerR{
  box-sizing: border-box;
}
.centerR .centerRT{
  width:140rpx;
  height:46rpx;
  background:rgba(178,136,80,1);
  border-radius:23rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}
.centerRTm{
  margin-bottom: 20rpx;
}
.withdrawB{
  width: 100%;
  height:80rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  padding-left: 24rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-weight:bold;
  color:rgba(51,51,51,1);
  font-size:26rpx;
  margin-top: 13rpx;
}
.withdrawB .withdrawBIcon{
  width: 29rpx;
  height: 29rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1rpx solid rgba(51,51,51,1);
  margin-right: 15rpx;
  border-radius: 50%; 
}