/* pages/commonInfo/commonInfo.wxss */
.common{
  width: 100%;
  box-sizing: border-box;
}
.commonBox{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: fixed;
  background:rgba(249,249,248,1);
  padding: 20rpx 24rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  overflow: auto;
}
.commonBox .item{
  width: 100%;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
  box-sizing: border-box;
  padding: 25rpx 25rpx 19rpx;
  margin-bottom: 19rpx;
  position: relative;
}
.commonBox .item .title{
  width: 100%;
  box-sizing: border-box;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 21rpx;
  border-bottom: 2rpx dashed rgba(102,102,102,1);
}
.commonBox .item .title .txtMr{
  font-size:22rpx;
  color:rgba(153,153,153,1);
}
.commonBox .item .title .setTxt{
  font-size:22rpx;
  color:rgba(238,107,17,1);
  /* position: absolute;
  right: 25rpx;
  top:8rpx;
  padding: 20rpx;
  z-index: 99; */
}
.commonBox .item .itemBox{
  width: 100%;
  box-sizing: border-box;
  margin-top: 22rpx;
}
.commonBox .item .itemBox .itemBoxs{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 10rpx;
}
.footer{
  width: 100%;
  height:98rpx;
  background:rgba(255,255,255,1);
  border-top:1rpx solid rgba(229,229,229,1);
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 33
} 
.footer .footerBtn{
  box-sizing: border-box;
  width:500rpx;
  height:64rpx;
  background:rgba(255,95,0,1);
  border-radius:32rpx 32rpx 32rpx 0px;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  display: flex;
  justify-content: center;
  align-items: center;
}
.demo-item{
  width: 100%;
  height: 800rpx;
}