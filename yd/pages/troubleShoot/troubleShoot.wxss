/* pages/troubleShoot/troubleShoot.wxss */
.troubleShootingBox{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: fixed;
  background:rgba(249,249,248,1);
  overflow: auto;
}
.troubleShooting{
  width: 100%;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  padding: 32rpx 29rpx;
}
.item{
  margin-bottom: 32rpx;
}
.item .title{
  font-size:32rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
  margin-bottom: 14rpx;
}