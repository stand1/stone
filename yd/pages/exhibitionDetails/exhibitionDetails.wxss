/* pages/exhibitionDetails/exhibitionDetails.wxss */
.exhDetailsBox{
  width: 100%;
  box-sizing: border-box;
}
.exhDetails{
  width: 100%;
  box-sizing: border-box;
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:400;
  background:rgba(238,238,238,1);
  padding-bottom: 126rpx;
}
.exhHeader{
  width: 100%;
  height:429rpx;
  box-sizing: border-box;
  background:radial-gradient(circle,rgba(2,12,74,1) 0%,rgba(255,255,255,1) 100%);
  opacity:0.73;
  padding: 39rpx 24rpx 37rpx;
  display: flex;
  color:rgba(255,255,255,1);
}
.exhHeaderL{
  width:380rpx;
  height:353rpx;
  background:rgba(0,0,0,1);
  box-sizing: border-box;
  margin-right: 30rpx;
}
.exhHeaderL image{
  width:380rpx;
  height:353rpx;
}
.exhHeaderR{
  width: 100%;
  height: 353rpx;
  box-sizing: border-box;
  position: relative;
}
.exhHeaderR .exhHeaderRT .title{
  font-weight:bold;
  font-size: 32rpx;
  padding: 13rpx 0 26rpx;
}
.exhHeaderR .exhHeaderRB{
  position: absolute;
  left: 0;
  bottom: 21rpx;
}
.exhHeaderR .colors{
  font-weight:bold;
  color:rgba(251,192,0,1);
  font-size: 30rpx;
}
.exhName{
  width: 100%;
  height: 110rpx;
  box-sizing: border-box;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 22rpx 34rpx 24rpx 25rpx;
}
.exhNameL{
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
}
.exhNameL .exhNameTile{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
  margin-bottom: 14rpx;
}
.exhNameR{
  color:rgba(53,114,195,1);
  display: flex;
  align-items: center;
  height: 34rpx;
}
.exhNameR .exhNameRtxt{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 34rpx;
}
.exhNameRtxt image{
  width: 16rpx;
  height: 25rpx;
  margin-left: 15rpx;
}
.exhNameR .icons{
  color: #3572C3;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
}
.exhReserve{
  width: 100%;
  height: 110rpx;
  box-sizing: border-box;
  margin: 10rpx 0;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
  padding: 0 34rpx 0 24rpx;
}
.exhReserveR{
  color: #C7C7CC;
  font-size:38rpx;
  font-family:PingFang SC;
  font-weight:bold;
}
.exhReserveR image{
  width: 16rpx;
  height: 25rpx;
  box-sizing: border-box;
}
.projectDetails{
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  padding: 26rpx 33rpx 50rpx 25rpx;
}
.projectDetails img{
  width: 100%;
  box-sizing: border-box;
}
.exhFooter{
  width:100%;
  height:97rpx;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(229,229,229,1);
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 24rpx 0rpx 0rpx;
  box-sizing: border-box;
}
.exhFooterL{
  width: 40rpx;
  box-sizing: border-box;
  margin-left: 50rpx;
}
.share{
  box-sizing: border-box;
  width: 40rpx;
  padding: 0;
  margin-left: 0;
  margin-right: 0;
  background: #fff;
}
.share .shareImg{
  width: 40rpx;
  height: 79rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}
.share .shareImg image{
  width: 35rpx;
  height: 39rpx;
}
.share .shareTxt{
  width: 40rpx;
  box-sizing: border-box;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.exhFooterR{
  display: flex;
  text-align: center;
  color:rgba(255,255,255,1);
  font-weight:bold;
}
.exhFooterR .timers{
  width:228rpx;
  height:74rpx;
  background:linear-gradient(0deg,rgba(3,104,218,1) 0%,rgba(0,120,255,1) 100%);
  border-radius:37rpx 37rpx 37rpx 0rpx;
  color: #fff;
  box-sizing: border-box;
}
.exhFooterR .timers .timersT{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
}
.exhFooterR .closing{
  width:250rpx;
  height:74rpx;
  /* line-height: 74rpx; */
  background:rgba(255,113,0,1);
  border-radius:37rpx 37rpx 37rpx 0px;
  margin-left: 19rpx;
}
.closing button{
  background: rgba(255,113,0,1);
  margin: 0;
  padding: 0;
  color: #fff;
  width: 100%;
  box-sizing: border-box;
}
.exhFooterR .closing .closingT{
  font-size:28rpx;
}
.exhFooterR .reserve{
  color: #FFF;
  width:228rpx;
  height:69rpx;
  text-align: center;
  line-height: 69rpx;
  background:rgba(255,113,0,1);
  border-radius:35rpx 35rpx 35rpx 0px;
  margin-left: 15rpx;
  font-size:28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.exhFooterR .reserve .reserveTBtn{
  border: none;
  padding: 0;
  color: #fff;
  background:#FFA156;
  display: flex;
  justify-content: center;
  align-items: center;
}
.exhFooterR .reserves{
  background:#FFA156;
}
.exhFooterR .presell{
  width:342rpx;
  height:69rpx;
  /* line-height: 69rpx; */
  text-align: center;
  background:linear-gradient(0deg,rgba(9,156,8,1) 0%,rgba(26,173,25,1) 100%);
  border-radius:35rpx 35rpx 35rpx 0rpx;
}
.exhFooterR .presellT{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
}
.exhFooterR .soldOut {
  width: 702rpx;
  background: #868584;
  color: #FFF;
  height: 69rpx;
  line-height: 69rpx;
  border-radius: 40rpx 40rpx 40rpx 0rpx;
  box-sizing: border-box;
  margin-left: 24rpx;
  font-size: 32rpx;
  font-weight: bold;
  font-family:PingFang SC;
}
.exhFooterR .select{
  width:702rpx;
  height:69rpx;
  line-height: 69rpx;
  background:linear-gradient(0deg,rgba(3,104,218,1) 0%,rgba(0,120,255,1) 100%);
  border-radius:35rpx 35rpx 35rpx 0px;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  margin-left: 24rpx;
}
.exhFooterR .reserveed{
  width:702rpx;
  height:69rpx;
  line-height: 69rpx;
  background:linear-gradient(0deg,rgba(9,156,8,1) 0%,rgba(26,173,25,1) 100%);
  box-shadow:0px 1px 7px 0px rgba(1, NaN, 170, 0.35);
  border-radius:35rpx 35rpx 35rpx 0px;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  margin-left: 24rpx;
}
.exhFooterR .payment{
  width:702rpx;
  height:69rpx;
  line-height: 69rpx;
  background: linear-gradient(-90deg,rgba(250,125,65,1) 0%,rgba(251,176,80,1) 100%);
  box-shadow:0px 1px 7px 0px rgba(1, NaN, 170, 0.35);
  border-radius:35rpx 35rpx 35rpx 0px;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  margin-left: 24rpx;
}
.exhFooterR .refund{
  width:702rpx;
  height:69rpx;
  line-height: 69rpx;
  background:linear-gradient(180deg,rgba(238,139,143,1) 0%,rgba(239,124,124,1) 100%);
  box-shadow:0px 1px 7px 0px rgba(1, NaN, 170, 0.35);
  border-radius:35rpx 35rpx 35rpx 0px;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  margin-left: 24rpx;
}
.modleBox{
  width: 100%;
  box-sizing: border-box;
  padding: 37rpx 0 0;
}
.modleBoxCon{
  width: 100%;
  box-sizing: border-box;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(35,32,32,1);
  font-size: 22rpx;
  padding: 0 24rpx;
}
.modleBoxCons{
  margin-top: 20rpx;
}
.modleBoxCon .title{
  text-align: left;
  font-weight:bold;
  color:rgba(0,0,0,1);
  font-size:28rpx;
  margin-bottom: 25rpx;
}
.modleBoxCon .mianBox{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
}
.modleBoxCon .mianBox .item{
  background:rgba(238,238,238,1);
  border-radius:8rpx;
  padding: 16rpx 26rpx;
  margin-right: 15rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}
.modleBoxCon .mianBox .itemActive{
  background:rgba(255,217,190,1);
}
.modleBoxCon .mianBox .itemErr{
  background:#BFBFBF;
}
.selected{
  width:100%;
  height:60rpx;
  background:rgba(238,238,238,1);
  box-sizing: border-box;
  margin-top: 29rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(35,32,32,1);
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 26rpx;
}
.selected .title{
  font-size:22rpx;
  font-weight:400;
  color:rgba(35,32,32,1);
}
.selected .txt{
  margin: 0 18rpx 0 24rpx;
}
.medleBoxBtn{
  width:100%;
  height:97rpx;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(229,229,229,1);
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}
.medleBoxBtn .btn{
  width:500rpx;
  height:64rpx;
  line-height: 64rpx;
  background:rgba(255,95,0,1);
  border-radius:32rpx 32rpx 32rpx 0px;
}
